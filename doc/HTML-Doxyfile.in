GENERATE_XML           = NO
SEARCHENGINE           = NO
PDF_HYPERLINKS         = NO
USE_PDFLATEX           = NO
STRIP_CODE_COMMENTS    = NO

HTML_FILE_EXTENSION    = .html
#HTML_HEADER            = header.html
#HTML_FOOTER            = footer.html
#HTML_EXTRA_STYLESHEET  = custom.css
HTML_OUTPUT            = html

SEARCHENGINE		   = YES
SERVER_BASED_SEARCH    = NO
