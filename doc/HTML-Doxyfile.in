DOXYFILE_ENCODING      = UTF-8

PROJECT_NAME           = LandProcessor 
PROJECT_NUMBER         = 0.5 
#PROJECT_LOGO           = 

OUTPUT_DIRECTORY       = @DOC_OUTPUT_DIR@

FULL_PATH_NAMES        = YES
STRIP_FROM_PATH        = @DOC_SRCTOPARSE_DIR@
STRIP_FROM_INC_PATH    = @DOC_SRCTOPARSE_DIR@


EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = NO
EXTRACT_STATIC         = YES
